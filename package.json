{
  "name": "atomico-base",
  "description": "a repo to start with atomico",
  "meta": {
    "title": "Started",
    "description": "Hassle-free starter template, perfect for starting with Atomico",
    "branch": "https://github.com/atomicojs/atomico/tree/1-estack-blank/"
  },
  "version": "0.0.0",
  "scripts": {
    "start": "estack src/index.html docs --watch --server --sourcemap",
    "build": "estack src/index.html docs --watch --server --sourcemap --minify",
    "build:npm": "estack src/components/**/*-*.js dest --server --sourcemap",
    "create:component": "tfolder tfolder/component src/components",
    "create:hook": "tfolder tfolder/hook src/hooks"
  },
  "dependencies": {
    "atomico": "^0.23.3"
  },
  "devDependencies": {
    "estack": "^0.7.1",
    "tfolder-cli": "^0.2.0"
  }
}
